{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@pages/*": ["src/pages/*"],
      "@assets/*": ["src/assets/*"],
      "@layout/*": ["src/layout/*"],
      "@components/*": ["src/components/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    // "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": "./",
    "paths": {
      "@types": ["src/types"],
      "@utils": ["src/utils"],
      "@constants": ["src/constants"],
      "@api/*": ["src/api/*"],
      "@pages/*": ["src/pages/*"],
      "@assets/*": ["src/assets/*"],
      "@layout/*": ["src/layout/*"],
      "@content/*": ["src/content/*"],
      "@components/*": ["src/components/*"],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}